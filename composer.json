{
    "name": "entrili-oss/repository-doctrine-orm",
    "description": "Doctrine based ORM implementations of an Experience API (xAPI) repository",
    "keywords": ["xAPI", "Tin Can API", "Experience API", "storage", "database", "repository", "entity", "Doctrine", "ORM"],
    "homepage": "https://github.com/entrili-oss/repository-doctrine-orm/",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Flothmann",
            "homepage": "https://github.com/xabbuh"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/entrili-oss/repository-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entrili-oss/repository-doctrine"
        },
        {
            "type": "vcs",
            "url": "https://github.com/entrili-oss/model"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "doctrine/orm": "^2.3",
        "entrili-oss/repository-api": "^0.4",:
        "entrili-oss/repository-doctrine": "^0.3"
    },
    "require-dev": {
        "entrili-oss/model": "^2.0"
    },
    "provide": {
        "entrili-oss/repository-implementation": "0.3"
    },
    "autoload": {
        "psr-4": {
            "XApi\\Repository\\ORM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XApi\\Repository\\ORM\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
